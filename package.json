{
  "name": "root",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "lint-staged": {
    "./packages/**/*.js": [
      "eslint --cache --fix",
      "git add"
    ],
    "./packages/**/*.less": [
      "stylelint --cache --fix",
      "git add"
    ]
  },
  "scripts": {
    "start": "start-storybook",
    "build": "build-storybook -c .storybook -o docs",
    "prebuild": "rm -rf docs; mkdir docs;"
  },
  "browserslist": [
    "iOS >= 8",
    "Android >= 4.1"
  ],
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.5.5",
